import java.util.Scanner;
public class Main {
    public static void main (String[] args){
        Aluno aluno = new Aluno("12345", 20.0);

        System.out.println("Matricula: " + aluno.getMatricula() + " Nota: " + aluno.getNota());
        System.out.println(aluno);
        
        Aluno aluno;
        String matricula;
        double nota;
        int qtdFichas = 100;
        Scanner entrada = new Scanner(System.in);
        
        for (int i = 0; i < qtdFichas; i++){
            System.out.println("Informe a matricula do " + i + " aluno.");
            matricula = entrada.nextLine();
            System.out.println("Informe a nota do " + i + 1 + " aluno.");
            nota = entrada.nextInt();
        }
    }
}
